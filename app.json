{
  "expo": {
    "name": "IAe, Bora?",
    "slug": "iaebora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.iaebora.optime",
      "versionCode": 12,
      "config": {
        "googleSignIn": {
          "apiKey": "<GOOGLE_APIKEY>",
          "certificateHash": "<GOOGLE_FINGERPRINT>"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
